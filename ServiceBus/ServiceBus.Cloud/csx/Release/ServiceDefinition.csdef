<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="ServiceBus.Cloud" schemaVersion="2014-06.2.4" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition">
  <WebRole name="ServiceBus.WebApi">
    <LocalResources>
      <LocalStorage name="DiagnosticStore" sizeInMB="4096" cleanOnRoleRecycle="false" />
    </LocalResources>
    <ConfigurationSettings />
    <Endpoints>
      <InputEndpoint name="Endpoint1" protocol="http" port="80" localPort="80" />
    </Endpoints>
    <Certificates />
  </WebRole>
  <WorkerRole name="ServiceBus.Recive">
    <LocalResources>
      <LocalStorage name="DiagnosticStore" sizeInMB="4096" cleanOnRoleRecycle="false" />
    </LocalResources>
    <ConfigurationSettings>
      <Setting name="FlushDbBallsCount" />
      <Setting name="LogQueueCount" />
      <Setting name="LogQueueName" />
      <Setting name="LogSqlConnectionString" />
      <Setting name="MaxConcurrentCalls" />
      <Setting name="Microsoft.ServiceBus.ConnectionString" />
      <Setting name="ReceiveBatchCount" />
      <Setting name="Start" />
      <Setting name="TestSpeed" />
      <Setting name="TestSpeedQueueCount" />
      <Setting name="ThreadPerQueue" />
      <Setting name="UseTrigger" />
    </ConfigurationSettings>
    <Endpoints />
    <Certificates />
  </WorkerRole>
</ServiceDefinition>